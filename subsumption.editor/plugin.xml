<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="subsumption.presentation.SubsumptionModelWizardID"
            name="%_UI_SubsumptionModelWizard_label"
            class="subsumption.presentation.SubsumptionModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SubsumptionModelFile.gif">
         <description>%_UI_SubsumptionModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="subsumption.presentation.SubsumptionEditorID"
            name="%_UI_SubsumptionEditor_label"
            icon="icons/full/obj16/SubsumptionModelFile.gif"
            extensions="subsumption"
            class="subsumption.presentation.SubsumptionEditor"
            contributorClass="subsumption.presentation.SubsumptionActionBarContributor">
      </editor>
   </extension>

</plugin>
